<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>



    <rule>
        <from>.*index_unit_test.jsp</from>
        <to last="true">-</to>
    </rule>
    
    <rule>
        <from>^/admin.version</from>
        <to type="redirect" last="true">%{context-path}/rest/administration/version</to>
    </rule>

    <rule>
        <from>^.*/(rest|mock|css|fonts|images|sound|js)/.*$</from>
        <to last="true">-</to>
    </rule>

    <rule>
        <from>.*token.*$</from>
        <to last="true">-</to>
    </rule>
    <rule>
        <from>^/?page=.*$</from>
        <to last="true">-</to>
    </rule>
    <rule>
        <from>^/$</from>
        <to last="true">-</to>
    </rule>

    <rule>
        <from>^/(.*)</from>
        <to type="redirect">%{context-path}/?page=$1</to>
    </rule>


</urlrewrite>

